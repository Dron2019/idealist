    style.
        .preloader{
            padding:60px 0;
            transition:.7s;
            background:#fff;
        }
        .welcome{
            color:white;
            font-weight: bold;
            font-size: 18px;
            line-height: 18px;
            /* identical to box height, or 100% */

            text-align: center;
            letter-spacing: 1em;
            text-transform: uppercase;
        }
        .big-title {
            color:white;
        font-weight: 900;
        font-size: 48px;
        line-height: 48px;
        /* identical to box height, or 100% */

        text-align: center;
        letter-spacing: 0.2em;
        text-transform: uppercase;
        }
        .preload-num {
            font-weight: bold;
            font-size: 18px;
            line-height: 18px;
        }
        .preload-text-group {
            height: 135px;
            justify-content: space-between;
            display:flex;
            flex-direction:column;
            margin-top:auto;
            margin-bottom:auto;
            align-items:center;
        }
        .preload-background {
            position:absolute;
            top:0;
            right:0;
            height:100%;
                width: 50%;
            object-fit: cover;
            z-index: -1;
        }
    script.
        window.addEventListener('load', () => {
            document.querySelector('.preloader').style.opacity = '0';
            document.querySelector('.preloader').style.pointerEvents = 'none';
        })
        window.addEventListener('beforeunload', () => {
            document.querySelector('.preloader').style.transition = '0';
            document.querySelector('.preloader').style.opacity = '1';
            document.querySelector('.preloader').style.pointerEvents = 'none';
        });
        function animNumber(el) {
            var incrementor = 0;
            function increase(incrementor) {
            setTimeout(() => {
                incrementor++;
                el.innerHTML = incrementor;
                if (incrementor>=100)  {return};
                window.requestAnimationFrame(increase.bind(null,incrementor));
                
            }, 1000/60);
        }
        increase(incrementor);
        }
